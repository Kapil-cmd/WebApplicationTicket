@model IEnumerable<Repository.Entites.UserRole>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<table class="table table-striped table-responsive">
    <thead>
        <tr>
           
            <th>UserName</th>
            <th>Roles</th>
           
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
           
           <td>item.UserName</td>
           <td>item.Role</td>
           
        </tr>
}
    </tbody>
</table>
